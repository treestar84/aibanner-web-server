{
  "configuration": {
    "rsshub_domain": "https://rsshub.app",
    "daily_target": 20,

    "_type_descriptions": {
      "rss": "Standard RSS feed (raw content)",
      "atom": "Atom feed (typically GitHub releases)",
      "rsshub": "RSSHub proxy feed",
      "curated_rss": "Upstream AI-curated RSS (Stage 1+)",
      "link": "Web page crawling",
      "code": "GitHub README parsing",
      "github_md_folder": "GitHub folder latest .md file fetcher (Stage 1.5)",
      "github_json": "GitHub JSON file fetcher (ai-news-daily date-based aggregator)"
    },

    "deduplication": {
      "enabled": true,
      "method": "hybrid_url_then_embedding",
      "canonical_url_fields": ["link", "guid"],
      "embedding_similarity_threshold": 0.93,
      "metadata_fields": ["title", "source", "category"],
      "schedule": "daily_00:00_utc"
    },

    "filtering": {
      "enabled": true,

      "hard_drop_rules": {
        "title_or_content_contains_any": [
          "sponsored",
          "press release",
          "webinar",
          "conference tickets",
          "earnings call transcript",
          "giveaway"
        ],
        "url_contains_any": [
          "utm_source=",
          "utm_medium=",
          "ref="
        ]
      },

      "topic_whitelist_keywords": [
        "agent",
        "agents",
        "tool use",
        "tool-use",
        "MCP",
        "function calling",
        "computer use",
        "coding agent",
        "vibe coding",
        "RAG",
        "vector",
        "inference",
        "serving",
        "latency",
        "throughput",
        "GPU",
        "CUDA",
        "quantization",
        "speculative decoding",
        "KV cache",
        "LLM",
        "multimodal",
        "realtime",
        "voice",
        "speech",
        "o1",
        "reasoning",
        "benchmark",
        "eval",
        "open weight",
        "open-source",
        "release",
        "changelog"
      ],

      "soft_penalty_keywords": [
        "opinion",
        "hot take",
        "meme",
        "rumor",
        "leak",
        "celebrity",
        "lawsuit"
      ],

      "arxiv_paper_filter": ["cs.AI", "cs.LG", "cs.CL", "cs.SE"],
      "reddit_quality_filter": true,
      "devto_relevance_filter": true
    },

    "selection": {
      "enabled": true,
      "mode": "global_top_n",
      "top_n": 20,

      "llm_tagging": {
        "enabled": true,
        "output_schema": {
          "topic": "one_of(Model,Agent,VibeCoding,DevTool,Infra,Policy,Korea,Research,Other)",
          "impact": "int_1_to_5",
          "novelty": "int_1_to_5",
          "proof": "int_1_to_5",
          "confidence_score": "int_1_to_10",
          "why_it_matters": "short_string",
          "key_evidence": "short_string",
          "who_should_care": "short_string",
          "next_action": "optional_string",
          "comparison": "optional_string",
          "drop_reason": "optional_short_string"
        },
        "drop_if": {
          "topic_in": ["Other"],
          "impact_lte": 2,
          "proof_lte": 2,
          "confidence_score_lte": 3,
          "content_quality": {
            "summary_min_chars": 200,
            "insight_min_filled": 2,
            "insight_min_chars_each": 15
          }
        }
      },

      "scoring": {
        "formula": "0.35*impact + 0.25*novelty + 0.25*proof + 0.15*recency",
        "recency": {
          "half_life_hours": 36
        },
        "penalties": [
          { "if_title_or_content_contains_any": ["press release", "sponsored"], "subtract": 2.0 },
          { "if_title_or_content_contains_any": ["minor", "typo", "chore", "docs only"], "subtract": 1.0 },
          { "if_title_or_content_contains_any": ["alpha", "beta", "rc", "nightly", "dev"], "subtract": 0.8 }
        ]
      },

      "diversity_quotas": {
        "min": {
          "Model": 2,
          "Agent": 2,
          "VibeCoding": 1,
          "DevTool": 1,
          "Infra": 1,
          "Korea": 2
        },
        "max": {
          "Research": 3,
          "Policy": 3
        }
      },

      "source_weights": {
        "P0_CURATED": 1.25,
        "P0_RELEASES": 1.15,
        "P1_CONTEXT": 1.0,
        "P2_RAW": 0.85,
        "COMMUNITY": 0.7
      }
    }
  },

